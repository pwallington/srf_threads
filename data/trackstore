	<script type="text/javascript">
	
		
		var speed = 360;
		var halfSpeed = speed/2;
		var doubleSpeed = speed*2;
		
		var UI = {};
		
		
		UI.popupHelper = function(triggerID, popupID) {
			var trigger = '#'+triggerID;
			var popup = '#'+popupID+'';
			
			$(popup).hover(function(){
				$(popup).stop().animate({
					opacity: '1'
				},speed);
			});
		}
		
		
		UI.highlightTab = function(tab) {
		$(tab).addClass("ui_tab_highlighted");	
			Cufon.replace(tab, {textShadow: '#ffffff 0 1px'});
			$(tab).append('<div class="highlighted_filler" />');
			$('.highlighted_filler').css({
				width: $(tab).width() + 38
			});
		}
		UI.removeHilightTab = function(tab) {
			$(tab).removeClass("ui_tab_highlighted");
			$('.highlighted_filler').remove();
			Cufon.replace(tab);
		}
		
		
		UI.showModal = function(div) {
	
			var zDiv = $('#ui');
			var height = zDiv.height()+50;
			var width = zDiv.width()+10;
			var cHeight = $(div).height()+20;
	
			$(div).wrap('<div id="modal-background" />').wrap('<div id="modal-wrapper" />');
			$('#modal-background').css({
				width: width+'px',
				height: height+'px',
				top: "80px"
			});
			if($('#modal-background').height() < 400){
				$('#modal-background').height(350);
			}
			
			
			if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)) { //test for MSIE x.x;
				$('.modal').css({
					boxShadow: 'none'
				});
			}

			
			$('.modal').css({
				top: -height+'px'
			});
			$('#modal-background').fadeIn(600);
			$('.modal').animate({top:'60px'},speed);
			
			$(document).keyup(function(e) {
		  		if (e.keyCode == 27) { 
		  			UI.removeModal(); 
		  		}
			});
			
						
			
			setInterval(function(){
				$('#modal-background').height(zDiv.height()+50);
			},500);
			
			scrollTo(0,300);
		}
		UI.removeModal = function() {
			var height = $('.modal').height()+20;
			$('.modal').animate({top:-height+'px'},speed);
			$('#modal-background').fadeOut(500, function(){
				$('#modal-background').remove();
			});
		}
		
		UI.modalError = function(msg) {
			$('#modal-error').text(msg);
			$('#modal-error').slideDown(300);
			$('#modal-msg').slideUp(300);
			var error = el('modal-error');
				error.appendChild(element('a',{id:"close-error", href:"javascript:void(0)"}));
				$('#close-error').click(function(){
					$(this).fadeOut(halfSpeed, function(){
						$(this).remove();
						$('#modal-error').slideUp(halfSpeed);
					});
				});
		}
		
		UI.modalMSG = function(msg) {
			$('#modal-msg').text(msg);
			$('#modal-msg').slideDown(300);
			var error = el('modal-msg');
				error.appendChild(element('a',{id:"close-error", href:"javascript:void(0)"}));
				$('#close-error').click(function(){
					$(this).fadeOut(halfSpeed, function(){
						$(this).remove();
						$('#modal-msg').slideUp(halfSpeed);
						$(".modal *:not(#modal-error,#modal-msg)").slideDown();
					});
				});
		}
		
		
		UI.modalErrorWithNewlines = function(msg) {
			var container = el("modal-error");
			$(container).empty();
			container.appendChild(element("span", {innerHTML:msg}));
			var close = container.appendChild(element('a',{id:"close-error", href:"javascript:void(0)"}));
			$(close).click(function(){
				$(this).fadeOut(halfSpeed, function(){
					$(this).remove();
					$(container).slideUp(halfSpeed);
				});
			});			
			$(container).slideDown(300);
		}

		
		UI.handlePlaceholder = function() {

			if($('.modal').is(':visible')) {
				var selector = ".modal";
			} else {
				var selector = ".ui_tab_content";
			}
		
			var inputVal = $(selector+' input[type=text]').val();
		
			$(selector+' input[type=text]').focus(function() {
				if(inputVal != '' && $(this).val() == inputVal) {
					$(this).addClass('input-focus').val('');
				} else if($(this).val() != '' && $(this).val() != inputVal ) {
					$(this).addClass('modal-input-focus');
				}
			});
			
			$(selector+' input[type=text]').blur(function() {
				if($(this).val() == '' && $(this).hasClass('modal-input-focus')) {
					$(this).removeClass('input-focus').val(inputVal);
				} else if($(this).val() != '' && $(this).val() != inputVal ) {
					$(this).removeClass('input-focus');
				}
			});
			
			var textareaVal = $('.modal textarea').val();
			
			$('.modal textarea').focus(function() {
				$(this).css({
					color: '#000000',
					border: '1px solid #000000'
				});
			});
			
			$('.modal textarea').blur(function() {
				$(this).css({
					color: '#484848',
					border: '1px solid #999999'
				});
			});
			
			$('#reset-search').click(function() {
				$(selector+' input[type=text]').val(inputVal);
		
			});
		
		}

		
		UI.createFeedback = function(sessionId, div){ 
			// Edit this in order to change question information (Add/Remove Questions)
			var questionData = [
								{
									id:4,
									title: "Competition: ",
									description: "",
									numStars: 5
								},
								{
									id:3,
									title: "Enjoyment & Fun: ",
									description: "",
									numStars: 5
								},										
								{
									id:2,
									title: "Frame Rate: ",
									description: "",
									numStars: 5
								},
								{
									id:1,
									title: "Connection Quality: ",
									description: "",
									numStars: 5
								}
							];
			
			try {
				var createLeagueUI = el(div);
				   $('head').append('<link rel="stylesheet" type="text/css" href="/membersite/styles/instruction/custom-fonts.css?ver=2016.3.016">');
				var info = createLeagueUI.appendChild(element("div", {className: "feedbackForm"}, {width: "400px", margin: "auto", border: "thin solid #000", padding: "0 10px 10px", marginTop: "20px"}));
					info.appendChild(element("h2",{innerHTML:"How was this session?"})); 
					info.appendChild(element("div",{id:"feedback-modal-error", innerHTML:""}));
					info.appendChild(element("p", {innerHTML:"Please take a moment to answer these questions to help improve iRacing:"}));
					info.appendChild(element("p", {innerHTML:"Choose n/a for non-applicable answers."}));
					var infoList = info.appendChild(element("table", {className:"feedbackTable"}));
						var questionCount = 0;
						createForm(questionData); // Create feedback form	
						info.appendChild(element("a", {innerHTML: "Submit Feedback", className: "action-button green", id:"submitFeedback", href:"javascript:void(0);"}));
						
						// JS Closures Form/Create Stars
						function createForm(data){
							while((a=questionData.pop()) !== undefined){  
								var questionRow = infoList.appendChild(element("tr", {innerHTML: ""}));
								questionRow.appendChild(element("td",{innerHTML:a.title},{width:"200px", textAlign:"right",paddingRight:"10px"}));
								var ratingStars = questionRow.appendChild(element("td",{className: "sessionFeedbackRatings", innerHTML:a.description}));
								questionRow.appendChild(element("span",{innerHTML:""}));
								createStars(a, ratingStars);
								questionCount++;
							}  							
						}
						function createStars(a, container){
							// N/A default checked
							var label = container.appendChild(element("label",{innerHTML:"<span class=\"feedback-na active\">n/a</span>", title: "N/A", className: "feedback-starLbl"}));
							var input = label.appendChild(element("input", {type: "radio", name: "question"+a.id, className: "feedback-star question"+a.id, value: -1}));
							input.checked = true;
							
							// Create stars for each question
							for(var j = 1; j <= a.numStars; j++){
								var label = container.appendChild(element("label",{innerHTML:" ", title: j + "/" + a.numStars, className: "feedback-starLbl"}));
								var input = label.appendChild(element("input", {type: "radio", name: "question"+a.id, className: "feedback-star question"+a.id, value: j}));
							}
							$(".question"+a.id+"").click(function(){
								resetStar($(this).parents(".sessionFeedbackRatings").children('.feedback-starLbl:not(:first-child)')); // Revert all stars to outline
								setStar($(this).parents(".feedback-starLbl:not(:first-child)")); // Set the star for selected
								setStar($(this).parents(".feedback-starLbl").prevAll(".feedback-starLbl:not(:first-child)"));	// Set the number of previous stars for selected
								
								var RowNA = this.parentNode.parentNode.querySelectorAll(".feedback-na");
								// First one
								if (this.value == -1) {
									$(RowNA).addClass("active")
								}
								else {
									$(RowNA).removeClass("active")
								}
							});
							
							// Star Closures
							function resetStar(element){
								$(element).removeClass("icon-star").addClass("icon-star-outline");
							}
							function setStar(element){
								$(element).removeClass("icon-star-outline").addClass("icon-star");
							}
							
							// Star Closures [END]
							resetStar($(".sessionFeedbackRatings").children('.feedback-starLbl:not(:first-child)')); // Set star state (Uncheckec)
						}
						// JS Closures [END]
						
						$("#submitFeedback").click(function(){
							var questionOutput = [];
							for(var k = 1; k <= questionCount; k++){  
								if($('input[name="'+"question"+k+'"]:checked').val()){
									questionOutput[k-1] = parseInt($('input[name="'+"question"+k+'"]:checked').val());	
								}
							}
								 						
							$.ajax({
							    url : contextpath+"/member/GetSubsessionForMember?sessionID="+sessionId,
							    type: "GET",
							    data : {}, 
							    dataType : "JSON",
							    success: function(data, textStatus, jqXHR){
									var TheSSID = (Number(data) != -1) ? data : -1;		 						
									$.ajax({
									    url : "/membersite/member/SetConnectionFeedback",
									    type: "POST",
									    data : {sessionID:parseInt(TheSSID), questionAnswers:JSON.stringify(questionOutput)}, 
									    dataType : "JSON",
									    success: function(data, textStatus, jqXHR){
									        $(".feedbackForm").text("Thank you for your feedback!").css({"padding":"50px","textAlign":"center"});
									    },
									    error: function (jqXHR, textStatus, errorThrown){
									    	$("#feedback-modal-error").text("An error has occurred when submitting your feedback, please try again.").css({"color":"red","fontWeight":"bold"}); 
									    }
									});
							    },
							    error: function (jqXHR, textStatus, errorThrown){
							    	$("#feedback-modal-error").text("An error has occurred when submitting your feedback, please try again.").css({"color":"red","fontWeight":"bold"}); 
							    }
							});							
						});
			} catch(e) {};
		}


		
		UI.createPreloader = function(div, speedOverride){
			try {
				if (speedOverride != undefined) {
					speed = speedOverride;
				}
				
				UI.removeAllPreloaders();
				
				var selector = "#"+div;
				var width = $(selector).width();
				var height = $(selector).outerHeight();
				var position = $(selector).position();
				var preloader = div+"-preloader";
				
				$(selector).after("<div class='preloader-div' id='"+preloader+"' />");
				$('#'+preloader+'').append('<div />');
				$('#'+preloader+', #'+preloader+' div').css({
					display: "none",
					width: width+"px",
					height: height+"px",
					top: position.top+"px",
					left:position.left+"px"
				});
				
				$('#'+preloader+'').fadeTo(speed, 0.8, function(){
					$('#'+preloader+' div').fadeTo(speed, 0.7);
				});
	
				
				setTimeout("UI.removeAllPreloaders()", 30000);
			} catch(e) {};
		}

		UI.removePreloader = function(div){
			//console.log("removing preloader for " + div);
			var preloader = div+"-preloader";
			
			$('#'+preloader+' div').fadeTo(speed, 0, function(){
				$('#'+preloader+'').fadeOut(speed, function(){
					$(this).remove();
				});
			});
		}

		UI.removeAllPreloaders = function(){
			//console.log("> UI.removeAllPreloaders");
			$('.preloader-div').fadeOut(speed,function() {
				$(this).remove();
			});
			//console.log("< UI.removeAllPreloaders");
		}
			
	</script>
	
		
		<script type="text/javascript">
			
			/*
			console.group(" > Team Todos:")
				console.info(" - Hook Up The Create a Team Button, prevent 'Default Team Name'");
				console.info(" - Get Team Invites and Acceptance Rigged up.\nLooks like cancelling apps is set up.")
				console.info(" - Set up discussions for teams, auto refresh for new messages?")
				console.info(" - Get a new icon and button for team driving sessions on the 'Join Hosted Race' section");
				console.info(" - Hook up 'Team Sessions' Page to show team sesssions")
				console.info(" - Team car and suit painting")
			console.groupEnd();
			*/
			
			// It all starts with this
			var TeamUI = {};
			
			// -----========================================================
				// Set up a link to go to the "pre-populated" search results and show a team, using its ID or name
				TeamUI.linkToTeam = function(name,teamID) {
					if (name) {
						location.href = contextpath+"/member/MyTeamsAll.do?teamName="+escape(name);
					}
					if (teamID) {
						location.href = contextpath+"/member/MyTeamsAll.do?teamID="+escape(teamID);
					}
				}
				
				TeamUI.createTabs = function() {
					
					var containerBefore = el("pan2_col2_content");
						containerBefore.appendChild(element("a",{className:"large-button",id:"create-a-team", href:"javascript:TeamUI.createTeam();"}));
				
					var container = el("ui");
					
					var tabs = container.appendChild(element("div", {id:"ui_tabs"}));
						
						var myTeamsTab = tabs.appendChild(element("div", {id:"myTeamsTab", className:"pointer ui_tab euro", innerHTML:"My Teams"}));
							myTeamsTab.onclick = function() {window.location.href = contextpath + "/member/MyTeams.do";}
						
						var teamDirectoryTab = tabs.appendChild(element("div", {id:"teamDirectoryTab", className:"pointer ui_tab euro", innerHTML:"All Teams"}));
							teamDirectoryTab.onclick = function() {window.location.href = contextpath + "/member/MyTeamsAll.do";}
						
						var teamSessionsTab = tabs.appendChild(element("div", {id:"teamSessionsTab", className:"pointer ui_tab euro", innerHTML:"Team Sessions"}));
							teamSessionsTab.onclick = function() {window.location.href = contextpath + "/member/MyTeamsActive.do";}
				
						var myTeaminvitesTab = tabs.appendChild(element("div", {id:"myTeaminvitesTab", className:"pointer ui_tab ui_tab_last euro", innerHTML:"Team Invites"}));
							myTeaminvitesTab.onclick = function() {
								window.location.href = contextpath + "/member/MyTeamInvites.do";
							}
							
						Cufon.replace(tabs, {textShadow: '#dcdcdc 0 1px 0'});
				}
				
				TeamUI.createTeam = function() {
					var containerBefore = el("pan2_col2_content");
					var div = containerBefore.appendChild(element("div", {id:"createTeamDlg", className:"modal teamModal"}));
						var modalTitle = div.appendChild(element("div",{id:"create-a-team-title", className:"modal-title"}));
							modalTitle.appendChild(element("a",{id:"team-title",href:"javascript:UI.removeModal();"}));
				
						var createTeamUI = div.appendChild(element("div",{id:"createTeamUI"}));
					UI.showModal(div);
					
					TeamUI.showCreateTeamForm(0);
					
				}
				
				function createTeamUIHandler(req){
					return function(){
						if (req.readyState==4) {
			  				if (req.status==200){
			  				var OrderData = extractJSON(req.responseText);
								if (OrderData && OrderData.length>0) {
									var amount_due  = OrderData[0].amount_due;
									var order_amount = OrderData[0].order_amount;
									if(amount_due > 0) {
										TeamUI.showRechargeRequired(amount_due);
									} else {
										TeamUI.showCreateTeamForm(order_amount);
									}
								}
							}
						}
					}
				}
				
				TeamUI.showRechargeRequired = function(amount_due){
				
					var createTeamUI = el("createTeamUI");
						removeAllChildren(UI);
						createTeamUI.appendChild(element("h2",{innerHTML:"We're Sorry.."}));
					
					var info = createTeamUI.appendChild(element("div"));
						info.appendChild(element("p",{innerHTML:"You currently do not have enough iRacing credits to register a new Team. Please purchase more iRacing credits to proceed."},{}));
					
					var actions = createTeamUI.appendChild(element("div",{className:"last-modal-div"}));
						actions.appendChild(element("a",{className:"toolbar-button-left", href:"javascript:UI.launchRecharge()",innerHTML:'<img src="https://d3bxz2vegbjddt.cloudfront.net/members/member_images/ui/icons/save.png"/>Recharge'},{marginLeft:"67px"}));
						actions.appendChild(element("a",{className:"toolbar-button-left", href:"javascript:UI.removeModal()",innerHTML:'<img src="https://d3bxz2vegbjddt.cloudfront.net/members/member_images/ui/icons/cancel.png"/>Cancel'}));
						actions.appendChild(element("p",{innerHTML:"Costs $"+amount_due+".00/yr <a href='javascript:javascript:openTermsWin()'>Click Here</a> for Terms of Use"},{textAlign:"center"}));
							
				}
		
				TeamUI.showCreateTeamForm = function(order_amount){
				
					var createTeamUI = el("createTeamUI");
					removeAllChildren(createTeamUI);
					
					createTeamUI.appendChild(element("h2",{innerHTML:"Start Your Team Today!"}));
					createTeamUI.appendChild(element("div",{id:"modal-error", innerHTML:""}));
					
					var info = createTeamUI.appendChild(element("div"));
						info.appendChild(element("p", {innerHTML:"It's never been easier to manage a racing Team on iRacing.com."}));
						var infoList = info.appendChild(element("ul"));
							infoList.appendChild(element("li",{innerHTML:"Define your own team colors"}));
							infoList.appendChild(element("li",{innerHTML:"Register for races as a team"}));
							infoList.appendChild(element("li",{innerHTML:"Chat live with your team mates"}));
					
					var TeamCreate = createTeamUI.appendChild(element("div",{className:"last-modal-div"}));
						var form = TeamCreate.appendChild(element("form",{},{paddingLeft:"25px"}));
							form.onsubmit = function() {
								TeamUI.createTeamSubmit();
								return false;
							}
							form.appendChild(element("input",{type:"text",className:"no-right-radius",id:"TEAM_NAME", name:"TEAM_NAME","placeholder":"Enter Team Name"},{width:"195px"}));
							UI.handlePlaceholder();
							form.appendChild(element("a",{className:"toolbar-button-left", href:"javascript:TeamUI.createTeamSubmit();",innerHTML:'<img src="https://d3bxz2vegbjddt.cloudfront.net/members/member_images/ui/icons/save.png"/>Save'},{position:"relative",marginLeft:"-1px",marginTop:"9px"}));
							$(document).keypress(function(e) {
								if($('#TEAM_NAME').hasClass('input-focus')) {
								    if(e.which == 13) {
								       e.preventDefault();
								       TeamUI.createTeamSubmit();
								    }
							    }
							});	
				}
				
				TeamUI.launchRecharge = function(){
					UI.removeModal();
					handleRecharge();
				}
				
				TeamUI.createTeamSubmit = function() {
					var error_display = el("modal-error");
					error_display.innerHTML="";
					var TeamName = el("TEAM_NAME").value;
					
					// Does this look like an email address?
					if (ValidateEmailAddress(TeamName)) {
						UI.modalError("Team names that look like email addresses aren't allowed. Please try again.");
						return false;
					}
					
					// Limit number of teams
					if (!(TeamOwnership < maxTeamsPerMember)) {
						UI.modalError("You are allowed a maximum of "+maxTeamsPerMember+" teams. Please delete one, then try again.");
						return false;
					}
					
					// Make sure team name is entered...
					if (!((TeamName != "Enter Team Name") && (TeamName.length > 0))) {
						UI.modalError("There is an error with your team name. Please try again.");
						return false;
					};
					
					var parms = {};
					parms['name'] = TeamName;
					load(contextpath+"/member/CreateTeam",parms,createTeamSubmitHandler);
				}
				
				function createTeamSubmitHandler(req){
					return function(){
						if (req.readyState==4) {
				  			if (req.status==200){
				  				success = extractJSON(req.responseText);
				  				if(success!=0){			
									window.location = contextpath + "/member/MyTeams.do?teamID="+success;
								} else {
									UI.modalError("There was a problem creating this team. Please wait a moment, then try again.");
								}
								
							}
						}
					}
				}
				TeamUI.showHideReplies = function(link) {
		
						if($('#'+link+'showHide').text()=='Show Replies'){
							$('#'+link+'showHide').text('Hide Replies');
						} else {
							$('#'+link+'showHide').text('Show Replies');
						}
						$('#'+link+'replies').animate({
							height: 'toggle',opacity: 'toggle'
						},speed);
		
				}
				TeamUI.showHideReplyInput = function(link) {
		
		
						if($('#'+link+'showHideInput').text()=='Reply'){
							$('#'+link+'showHideInput').text('Cancel');
						} else {
							$('#'+link+'showHideInput').text('Reply');
						}
						$('#'+link+'input').animate({
							height: 'toggle',opacity: 'toggle'
						},speed);
		
				}
			// -----========================================================
		</script>
<link rel="stylesheet" href="/membersite/styles/page_css/tracks.css?ver=081820171012334" />



	<div class="eachpage_title">
		<div id="Title_Pages"></div>
		<script>
			if (location.search.toLowerCase().indexOf("unowned") !== -1 && location.search.toLowerCase().indexOf("forceload") !== -1) {
				document.getElementById("Title_Pages").innerHTML = "BUY TRACKS";
				document.getElementById("Title_Pages").appendChild(element("a", {"innerHTML": "My Tracks", "href": contextpath + "/member/Tracks.do?forceload=owned"}, {"position": "absolute", "top": "0", "right": "0"}))
			} else if (location.search.toLowerCase().indexOf("owned") !== -1 && location.search.toLowerCase().indexOf("forceload") !== -1) {
				document.getElementById("Title_Pages").innerHTML = "MY TRACKS";
				document.getElementById("Title_Pages").appendChild(element("a", {"innerHTML": "Buy Tracks", "href": contextpath + "/member/Tracks.do?forceload=unowned"}, {"position": "absolute", "top": "0", "right": "0"}))
			} else {
				location = location.pathname + "?forceload=owned";
			}
		</script>
	</div>
	
	<div id="ui">
		<div class="ui_content">
			<div id="toolbar-top" class="clearfix">
				<h1 id="tracks-caption" class="floatleft">Tracks</h1>
				<div id="track-filters" class="floatright clearfix">
					<div class="floatleft hidden">
						<input type="checkbox" id="toggle-is-owned" /> <label for="toggle-is-owned">Owned</label>
					</div>
					<div class="floatleft hidden">
						<input type="checkbox" id="toggle-is-unowned" /> <label for="toggle-is-unowned">Unowned</label>
					</div>
					<div class="floatleft" id="track-search-container">
						<input type="text" class="search-tracks" placeholder="Search Tracks..." />
						<a href="javascript:void(0);" onclick="TrackPage.LiveFilter('');" id="clear-search-tracks"><span class="icon icon-cancel-circle"></span></a>
					</div>
				</div>
			</div>
			<div id="full_width_content" style="height: auto; overflow: hidden; background: url(https://d3bxz2vegbjddt.cloudfront.net/members/member_images/ui/lines.png);"></div>
		</div>
	</div>
	
	<script type="text/javascript">
		// Open object
		TrackPage = {};
		TrackPage.data = {};
		TrackPage.Filters = {
			"owned":	true,
			"unowned":	true
		};
		
		TrackPage.Mode = function() {
			if (location.search.toLowerCase().indexOf("unowned") !== -1) {
				return "unowned";
			} else if (location.search.toLowerCase().indexOf("owned") !== -1) {
				return "owned";
			}
		}
		
		
		
		TrackPage.ForceLoad = "owned";
		
		//
		TrackPage.data.content_group_arr = new Array();
			if (TrackPage.Mode() === "owned") TrackPage.data.content_group_arr.push({group:0,tracks:[],"title":"Owned","stripecolor":"green"})
