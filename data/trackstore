	function parseMap(data) {
		var a,b;
		var c = {};
		data = data.replace(/{/,"").replace(/}/,"");
		if (data.length) {
			a = data.split(", ");
			for (i=0;i<a.length;i++) {
				b = a[i].split("=");
				eval("c."+b[0].replace(/\s/gi,"SPACEREPLACE")+" = "+Number(b[1]));
			};
		};
		return c;
	};
	// Look up Object Mappings
	function mappingsLookup(key,object) {
		returnStr = "";
		for (k in object) {
			if (key == eval(eval("object."+k))) {
				returnStr = k;
			};
		};
		return returnStr;
	}
