		// Live Filter
		TrackPage.LiveFilterTimer = null;
		TrackPage.LiveFilterString = "";
		TrackPage.LiveFilter = function(text) {
			clearTimeout(TrackPage.LiveFilterTimer);
			UI.removeAllPreloaders();
			TrackPage.LiveFilterTimer = setTimeout(function() {
				$(".no-search-results").remove();
				
				if ((TrackPage.LiveFilterString != text) && (text != "")) {
					$("#track-search-container  #clear-search-tracks").show();
					UI.createPreloader("ui");
					$.each($(".track-container .track-title-toggle"), function(i,each) {
						var caption = $(each).text();
						var match = eval("caption.match(/"+text+"/gi)");
						if (match && match.length) {
							$(each).parent(".track-title").parent(".track-container").show();
						} else {
							$(each).parent(".track-title").parent(".track-container").hide();
						}
					});
					$.each($(".track-group"), function(i,each) {
						
						if ($(each).children(".track-container:visible").size() == 0) {
							$("<div>",{
								"class"		: "no-search-results track-container",
								"html"		: "<p class=\"text-align-center\"><span class=\"icon icon-stop\"></span> No tracks matching that filter found</p>"
							}).appendTo(each);
						}
					})
					
					UI.removeAllPreloaders();
				} else {
					$("#track-search-container input").val("")
					$("#track-search-container  #clear-search-tracks").hide();
					TrackPage.filterDown();
				}
				TrackPage.LiveFilterString = text;
				
			}, 500);
		}
		
		// Filters (Object of filter toggles in TrackPage.Filters)
		TrackPage.FilterContent = function() {
			return TrackPage.data.content_group_arr;
		};
		
		// When clicking checkies, filter down
		TrackPage.filterDown = function() {
			var filterOptions = {
				"owned"				: $("#toggle-is-owned").is(":checked"),
				"unowned"			: $("#toggle-is-unowned").is(":checked")
			};
			
			var toShow = "";
			var toHide = "";
				if (filterOptions.owned == true) { toShow += ".track-container.owned," } else { toHide += ".track-container.owned,";}
				if (filterOptions.unowned == true) { toShow += ".track-container.unowned," } else { toHide += ".track-container.unowned,";}
			
			$(toShow).show()
			$(toHide).hide()
			
			$(".no-search-results").remove();
			$.each($(".track-group"), function(i,each) {
				
				if ($(each).children(".track-container:visible").size() == 0) {
					$("<div>",{
						"class"		: "no-search-results track-container",
						"html"		: "<p class=\"text-align-center\"><span class=\"icon icon-stop\"></span> No tracks matching that filter found</p>"
					}).appendTo(each);
				}
			})
		};
		
		TrackPage.CollapseSection = function(sectionID) {
			$(".group-"+sectionID+" .track-container").slideToggle();
			$(".collapse-"+sectionID).children("span").toggleClass("icon-triangle-down").toggleClass("icon-triangle-up");
		};
		
		TrackPage.ToggleDetails = function(ID) {
			// Flip +/- sign
			$(".toggle-icon-"+ID).children("span").toggleClass("icon-plus-circle").toggleClass("icon-minus-circle");
			
			// Turn of opacity
			$(".details-"+ID).parent(".track-container").toggleClass("open")
			
			// Animate show/hides
			if ($(".track-overview-"+ID).height() > 1) {
				$(".track-overview-"+ID).animate({"height":"0px"},"fast",function() {
					// Calculate
					var height = 0;
						height += 10; // padding
						
						var leftside = Number($(".details-"+ID+" .track-specific-details").height());
						
						var rightside = 150*Number($(".details-"+ID+" .one-track-config").size()); // Number of items
						
						if (Number(leftside) >= Number(rightside)) {
							height += Number(leftside);
						} else {
							height += Number(rightside);
						}
						
					$(".details-"+ID).animate({"height":height+"px"},"slow");
				});
			} else {
				
				$(".details-"+ID).animate({"height":"0px"},"fast",function() {
					$(".track-overview-"+ID).animate({"height":"95px"},"slow");
				});
			};
		};
		
		//
		TrackPage.addtocart_tracks_handler = function(req) {
			return function(){
				if(req.readyState == 4) {
					if(req.status == 200){
						try {
							eval("var ajax_response="+req.responseText.replace(/\+/g," ")+";");
						}
						catch(a){
							iRacingAlerts("Could not add item to cart. Please wait a moment, then try again. If this problem persists, please contact support.  (Error TP-01)");
							return;
						}
						
						if (ajax_response!=-1) {
							var cartskus=ajax_response.split(",");
							
							$.each(cartskus, function(i,each) {
								$(".sku-"+each).children(".track-title").children(".add-to-cart-link").html("In Cart <span class=\"icon icon-cart\"></span>").unbind("click").click(function() {
									javascript:viewcart();
								});
							});
							
						} else {
							iRacingAlerts("Could not add item to cart. Please wait a moment, then try again. If this problem persists, please contact support.  (Error TP-02)");
						}
					} else {
						iRacingAlerts("Could not add item to cart. Please wait a moment, then try again. If this problem persists, please contact support.  (Error TP-03)");
					}
				}
				UI.removeAllPreloaders(); 				
			}
		}

		// Render all Cars
		TrackPage.Render = function() {
			//
			UI.createPreloader("ui");
			
			// Clean
			$("#full_width_content").empty();
			
			// Make a new container
			var container = $("<div>",{"id":"content-"+i});
			
			// pkgID to track
			var pkgid;
			
			// Outer var to hit
			var details;
			
			// Loopmotron
			$.each(TrackPage.FilterContent(), function(i,group) {
				if (group.tracks.length) {
					var groupContainer = $("<div>",{"class":"track-group group-"+i}).appendTo(container);
					$("<h1>",{"html":group.title,"class":group.stripecolor+"-stripe stripe"}).appendTo(groupContainer);
					$("<a>",{"html":"<span class=\"icon icon-triangle-down\"></span>","href":"javascript:void(0);","class":"floatright collapse-toggle collapse-"+i}).click(function() {
						TrackPage.CollapseSection(i);
					}).appendTo(groupContainer)
					
					// Sort Alphabetically
					group.tracks = group.tracks.sort(sortByProp("name"));
					
					// Loop
					$.each(group.tracks, function(c,track) {
						// If first track in package grouping
						if (track.pkgID!=pkgid) {
							
							// make a bit of doable classes to filter on
							var addedclasses = "";
							if (track.owned == 1) { addedclasses += " owned"; } else { addedclasses += " unowned"; };
							if (track.discountGroupNames.match(/oval/gi)) { addedclasses += " is-oval-track"; } else { addedclasses += ""; };
							if (track.discountGroupNames.match(/road/gi)) { addedclasses += " is-road-track"; } else { addedclasses += ""; };
							
							var trackContainer = $("<div>",{"class":"track-container sku-"+track.sku+" "+addedclasses}).appendTo(groupContainer);
								// Title Bar
								var trackTitle = $("<div>",{"class":"track-title clearfix"}).appendTo(trackContainer);
									// Open/Close
									$("<a>",{"class":"track-info-toggle toggle-icon-"+track.trackID,"href":"javascript:void(0);","html":"<span class=\"icon icon-plus-circle floatleft\"></span>"}).click(function() {
										TrackPage.ToggleDetails(track.trackID);
									}).appendTo(trackTitle);
									// Name
									$("<a>",{"html":track.name,"href":"javascript:void(0);","class":"floatleft track-title-toggle"}).click(function() {
										TrackPage.ToggleDetails(track.trackID);
									}).appendTo(trackTitle);
									
									// Lonks
									if (!track.owned) {
										// If in the cart already
										if (skusincart.indexOf(track.sku)!=-1) {

											$("<span>",{"html":"($"+track.price+")","class":"floatright title-announcement"}).appendTo(trackTitle);
											$("<a>",{"html":"In Cart <span class=\"icon icon-cart\"></span>","class":"floatright title-announcement","href":"javascript:void(0);"}).click(function() {
												javascript:viewcart();
											}).appendTo(trackTitle);

										} else if ((track.freeWithSubscription == "true") && !MemBean.accountHasBeenPaid) {

											$("<a>",{"html":"Free With Upgrade <span class=\"icon icon-cart\"></span>","class":"floatright title-announcement","href":"/membersite/account/ChooseNewSubscription.do"}).appendTo(trackTitle);

										}else{
											$("<a>",{"html":"Add To Cart ($"+track.price+") <span class=\"icon icon-cart\"></span>","class":"floatright add-to-cart-link title-announcement","href":"javascript:void(0);"}).click(function() {
												$(this).html("Adding to cart...");
												addtocart('sku='+track.sku,TrackPage.addtocart_tracks_handler);
											}).appendTo(trackTitle);
										}

									} else {
										$("<a>",{"html":"Find Series &raquo;","href":"/membersite/member/Series.do?trackid="+track.trackID,"class":"floatright title-announcement"}).click(function() {
											selectTestTrack(track.trackID);
										}).appendTo(trackTitle);
									}
									
									// Indicators
									if (track.discountGroupNames.match(/oval/gi)) {
										$("<span>", {"class":"icon icon-oval type-icon floatright title-announcement","title":"This track is primarily used for OVAL racing"}).appendTo(trackTitle)
									}
									
									if (track.discountGroupNames.match(/road/gi)) {
										$("<span>", {"class":"icon icon-road type-icon floatright title-announcement","title":"This track is primarily used for ROAD racing"}).appendTo(trackTitle)
									}
									
									if (track.freeWithSubscription == "true") {
										$("<span>",{"html":"[INCLUDED]","class":"floatright title-announcement","title":"This content is included with paid iRacing subscription"}).appendTo(trackTitle)
									}
									
									// ---> Do free w/ sub
									
								// Details
								var trackDetails = $("<div>",{"class":"track-overview track-overview-"+track.trackID,"style":"background:url("+track.col_color_img+") 0 -20px no-repeat;"}).hover(function() {
									$(this).toggleClass("hover");
								}).click(function() {
									document.location.href = contextpath+"/member/TrackDetail.do?trkid="+track.trackID;
								}).appendTo(trackContainer);
								
								
									details = $("<div>",{
										"class"		: "clearfix details details-"+track.trackID
									}).css({"height":"0px"}).appendTo(trackContainer);
									
								//
								// Track Overview details
								var trackDetails = $("<div>",{"class":"clearfix track-specific-details"}).appendTo(details);
									
									// Track Logo
									$("<img>",{"class":"track-logo","src":track.exp_logo_img}).appendTo(trackDetails);
									// Track Website Lonk
									if (track.url) $("<a>",{"href":track.url,"target":"_blank","html":"Track Website","class":"track-link"}).appendTo(trackDetails);
									// Track Geographic Map
									$("<img>",{"class":"track-geo-map","src":track.exp_map_img}).appendTo(trackDetails);
								//
									$(TrackPage.BuildTrackSpecs(track)).appendTo(details);
						} else {
							// If n+1 track in package
							$(TrackPage.BuildTrackSpecs(track)).appendTo(details);
						}
						// Store
						pkgid=track.pkgID;
					});
				};
				
				
			});
			UI.removeAllPreloaders();
			return container;
		}
		
		TrackPage.BuildTrackSpecs = function(data) {
			// One track layout at a time
			var oneLayout = $("<div>",{"class":"clearfix one-track-config"});
					
				$("<img>",{"class":"floatleft","src":data.exp_config_img}).appendTo(oneLayout);
				
				var EXdetails = $("<div>",{"class":"expanded-track-details floatleft"}).appendTo(oneLayout);
					// Name
					$("<p>",{"class":"floatleft","html":"<strong>"+data.configname+"</strong>"}).appendTo(EXdetails)
					
					
					// Config
					$("<label>", {"html":"Laps for TT:","class":"floatleft"}).appendTo(EXdetails);
					$("<span>", {"html":data.nlapsSolo,"class":"floatleft value"}).appendTo(EXdetails);
					
					// Course Info
					$("<p>",{"class":"floatleft value","html":"<a href=\"/membersite/member/TrackDetail.do?trkid="+data.trackID+"&nocache="+new Date().getTime()+"\">Course Info</a>"}).appendTo(EXdetails);
					
					// Select Track
					if (data.owned) $("<p>",{"class":"floatleft value","html":"<a href=\"javascript:selectTestTrack("+data.trackID+");\">Select Track</a>"}).appendTo(EXdetails);
				
			return oneLayout;
		}
		
		TrackPage.Init = function() {
			UI.createPreloader("ui");
			
			// Force show?
			if (TrackPage.ForceLoad.length && (TrackPage.ForceLoad != "null")) {
				if (TrackPage.ForceLoad.match( /^.*\b(owned)\b.*$/  )) {
					MemPrefsListing.tracks.showOwnedtracks = Number(1);
				} else {
					MemPrefsListing.tracks.showOwnedtracks = Number(0);
				}
				if (TrackPage.ForceLoad.match( /^.*\b(unowned)\b.*$/  )) {
					MemPrefsListing.tracks.showUnownedtracks = Number(1);
				} else {
					MemPrefsListing.tracks.showUnownedtracks = Number(0);
				}
			}
			
			// Empty out
			$("#full_width_content").empty();
			
			// Render
			$(TrackPage.Render()).appendTo("#full_width_content");
			
			// Set up toggles
			$("#toggle-is-owned,#toggle-is-unowned").unbind("click");
				
				// Owned
				$("#toggle-is-owned").attr( ((MemPrefsListing.tracks.showOwnedtracks == 1)?"checked":"not-checked") , ((MemPrefsListing.tracks.showOwnedtracks == 1)?"checked":"")   ).click(function() {
					var pref = (($(this).is(":checked"))?1:0);
					MemPrefsListing.tracks.showOwnedtracks = Number(pref);
					sendMemberPreferenceToHost("memprefslisting.tracks.showOwnedtracks",Number(pref));
					TrackPage.filterDown();
				});
				
				// Unowned
				$("#toggle-is-unowned").attr( ((MemPrefsListing.tracks.showUnownedtracks == 1)?"checked":"not-checked") , ((MemPrefsListing.tracks.showUnownedtracks == 1)?"checked":"")   ).click(function() {
					var pref = (($(this).is(":checked"))?1:0);
					MemPrefsListing.tracks.showUnownedtracks = Number(pref);
					sendMemberPreferenceToHost("memprefslisting.tracks.showUnownedtracks",Number(pref));
					TrackPage.filterDown();
				});
				
				if (MemPrefsListing.tracks.showUnownedtracks == 1 && TrackPage.data.content_group_arr[0].tracks.length == 0) {
					$("<h1>",{"html":"<span class=\"icon icon-track\"></span> Congratulations, you own all the tracks!","class":"green-stripe stripe", "style": "cursor: pointer;"})
						.click(function() {
							location = contextpath+"/member/Tracks.do?forceload=owned";
						}).appendTo("#full_width_content");
				}
			
			// Set up live search
			$(".search-tracks").unbind("keyup");
			$(".search-tracks").keyup(function() {
				TrackPage.LiveFilter($(this).val());
			});
			
			// Filter down
			TrackPage.filterDown();
			UI.removeAllPreloaders();
		};
		
		$(document).ready(function() {
			TrackPage.Init();
		});
	</script>
							</div>
						</td>
					</tr>
				</table>
			</div>
		</td>
		<td style="padding:0px"></td>
	</tr>
	
	
	<tr>
		<td style="height:224px;padding:0px"></td>
		<td style="width:972px;height:224px;padding:0px;text-align:left">
			<div style="width:970px;height:224px;background-image:url('https://d3bxz2vegbjddt.cloudfront.net/members/member_images/footer/footer.gif');background-repeat:no-repeat;border-left:1px solid #4B4B4B;border-right:1px solid #4B4B4B">
		  
				<div style="position:relative;top:195px;margin-left:20px">
					<a href="javascript:openTermsWin()" class="footer_link">Terms of Use and End User License Agreement</a>
					<span class="padhorz5" style="color:white" >|</span>
					<a href="javascript:openPrivacyWin()"  class="footer_link" >Privacy Policy</a>
					<span class="padhorz5" style="color:#bbbbbb" >|</span>
					
